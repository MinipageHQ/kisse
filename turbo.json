{
  "pipeline": {
    "build": {
      "dependsOn": ["^build", "$DATABASE_URL"],
      "outputs": ["dist/**", ".next/**"]
    },
    "lint": {
      "outputs": []
    },
    "dev": {
      "cache": false
    }
  },
  "globalDependencies": [
    "$NEXT_PUBLIC_PLATFORM_SPACES_BASE",
    "$NEXT_PUBLIC_PLATFORM_VORE_BASE",
    "$NEXT_PUBLIC_PLATFORM_DOMAIN",
    "$NEXT_PUBLIC_PLATFORM_DOMAINS",
    "$NEXT_PUBLIC_TRANSLOADIT_AUTH_KEY",
    "$VERCEL_PROJECT_ID",
    "$VERCEL_TEAM_ID",
    "$AUTH_BEARER_TOKEN",
    "$SECRET",
    "$VERCEL_TEAM_ID",
    "$QUIRREL_BASE_URL",
    "$CLERK_API_KEY"
  ]
}
