{
  "name": "@saltanahq/kisse",
  "version": "1.0.0",
  "scripts": {
    "dev": "blitz dev",
    "build": "blitz build",
    "start": "blitz start",
    "studio": "blitz prisma studio",
    "lint": "eslint --ignore-path .gitignore --ext .js,.ts,.tsx .",
    "test": "jest",
    "test:watch": "jest --watch",
    "prepare": "husky install",
    "prisma": "blitz prisma",
    "prisma:seed": "blitz db seed",
    "prisma:push": "blitz prisma db push",
    "prisma:dev": "blitz prisma migrate dev",
    "vitro": "vitro",
    "build:vitro": "vitro build -o .vitrodist && mv ./.vitrodist ./public/",
    "dev:tunnel": "ssh -R 80:localhost:3000 localhost.run",
    "dev:db:reset": "blitza prisma migrate reset",
    "build:worker": "yarn run webpack --config ./worker.webpack.config.js",
    "start:worker": "node ./dist/worker.js",
    "start:worker:dev": "node -r dotenv/config ./dist/worker.js",
    "preinstall": "node ./deploy/create_npmrc_before_install.js",
    "postinstall": "rimraf ./db/zod && rimraf ./db/schema.prisma && cp ./node_modules/@saltanahq/database/prisma/schema.prisma ./db/schema.prisma && blitz prisma generate"
  },
  "prisma": {
    "schema": "db/schema.prisma",
    "seed": "db/seeds.ts"
  },
  "prettier": {
    "semi": false,
    "printWidth": 100
